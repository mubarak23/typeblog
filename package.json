    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.2.0",
    "mysql2": "^2.3.3",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
